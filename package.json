{
  "name": "hacksu-2021",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "cross-env NODE_ENV=production node server/index.js",
    "dev": "nodemon --watch server --watch db --ignore \"db/*.json\" server/index.js",
    "build": "vite-ssg build -c src/vite.config.js",
    "preview": "vite preview",
    "update": "git fetch && git pull && npm run build && pm2 restart hacksu"
  },
  "browser": {
    "markdown-it": false
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-session": "^1.17.3",
    "markdown-it": "^13.0.2",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-discord": "^0.1.4",
    "remult": "^0.22.9",
    "sharp": "^0.32.6",
    "vite-ssg": "^0.22.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "nodemon": "^3.0.1",
    "sass": "^1.26.5",
    "vite": "^4.4.11"
  }
}
